{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "globals": {
    "it": true,
    "describe": true
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "15.0"
    }
  },
  "rules": {
    "indent": [
      2,
      "tab"
    ],
    "id-length": 0,
    "react/jsx-indent": [
      2,
      "tab"
    ],
    "react/jsx-indent-props": [
      2,
      "tab"
    ],
    "no-unused-expressions": 0
  }
}